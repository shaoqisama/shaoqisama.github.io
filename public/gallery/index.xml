      <description>&lt;style&gt;
  
  .grid-gallery{max-width:100%}.grid-gallery .key-note{display:none;margin:0;padding:0 0 1px;font-size:80%;font-style:italic}.grid-gallery .key-note kbd{padding:.15em .35em;background-color:#ddd;color:#000;border-radius:.2em;font-family:monospace;font-size:inherit}.grid-gallery .key-note:not(:focus):not(:focus-visible):not(:active){clip:rect(0 0 0 0);clip-path:inset(50%);height:1px;overflow:hidden;position:absolute;white-space:nowrap;width:1px}.grid-gallery ul{display:flex;flex-wrap:wrap;justify-content:center;margin:0;padding:calc(var(--grid-gallery-image-active-outline-size)/2)0;background:var(--grid-gallery-background)}.grid-gallery li{flex-basis:100%;margin:calc(var(--grid-gallery-image-active-outline-size)/2);padding:0;list-style-type:none;transition-property:transform,opacity,outline-color;transition-timing-function:ease-in-out;transition-duration:var(--grid-gallery-duration-shrink);outline:calc(var(--grid-gallery-image-active-outline-size)/2)solid transparent}.grid-gallery ul.is-zoomed li{opacity:var(--grid-gallery-background-opacity)}.grid-gallery ul.is-zoomed li.is-zoomed{opacity:1}.grid-gallery li.is-zoomed{outline-color:var(--grid-gallery-image-active-outline-color);transition-duration:var(--grid-gallery-duration-expand)}.grid-gallery li.is-zoomed button,.grid-gallery ul:not(.is-zoomed) button:focus{outline-color:var(--grid-gallery-image-active-outline-color)}@media(prefers-reduced-motion){.grid-gallery li,.grid-gallery li.is-zoomed{transition-duration:0s}}.grid-gallery button{all:initial;display:grid;align-items:start;width:100%;overflow:hidden;cursor:pointer}.grid-gallery button:focus-visible{outline:calc(var(--grid-gallery-image-active-outline-size)/2)solid var(--grid-gallery-image-active-outline-color)}.grid-gallery figure{position:relative;margin:0;padding:0;aspect-ratio:var(--grid-gallery-image-aspect)}.grid-gallery figcaption{position:absolute;bottom:0;left:0;right:0;margin:0;padding:var(--grid-gallery-caption-padding);font-size:var(--grid-gallery-caption-font-size);color:var(--grid-gallery-caption-font-color);background:var(--grid-gallery-caption-background)}.grid-gallery img{height:100%;width:100%;object-fit:cover;margin:0;padding:0}@media screen and (min-width:400px){.grid-gallery .key-note{display:block}.grid-gallery li{flex-basis:calc( (100%/var(--grid-cols)) - calc(var(--grid-gallery-image-active-outline-size) * 1.5))}.grid-gallery li:hover{outline-color:var(--grid-gallery-image-active-outline-color)}}
&lt;/style&gt;
&lt;section
